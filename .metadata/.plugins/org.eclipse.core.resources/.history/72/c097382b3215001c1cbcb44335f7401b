package com.example.demo;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.CommandLineRunner;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.autoconfigure.batch.BatchProperties.Jdbc;
import org.springframework.jdbc.core.JdbcTemplate;

@SpringBootApplication
public class Spring2jdbc2postgresqlApplication implements CommandLineRunner {
    @Autowired 
	private JdbcTemplate jdbcTemplate;
	public static void main(String[] args) {
		SpringApplication.run(Spring2jdbc2postgresqlApplication.class, args);
	}
	@Override
	public void run(String... args) throws Exception {
		String sql="INSERT INTO emp (id,name,age,add,sal)" + "VALUES('04','raghu','24','patna','2866')";
		
	}

}
