package jsonExample;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileOutputStream;
import java.io.InputStream;
import java.io.OutputStream;

public class Day8a {
public static void main(String[] args) {
		
		
		try {
			InputStream in=new FileInputStream("E:\\InputJson.text");
			int i=0;
			while ((i=in.read())!=-1) {
				System.out.print((char)i);
				
			}
			in.close();
		}catch (Exception e) {
			e.printStackTrace();
		}

}
}


